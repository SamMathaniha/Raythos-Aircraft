@using Microsoft.AspNetCore.Identity
@model IdentityRole

@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Raythos Aerospace</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.1/css/all.min.css" integrity="sha384-..." crossorigin="anonymous">

    <!-- Custom fonts for this template-->
    <link href="~/vendorAdmin/fontawesome-free/css/all.min.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
          rel="stylesheet">

    <!-- Custom styles for this template-->

    <link href="~/css/sb-admin-2.min.css" rel="stylesheet" />
</head>

<body id="page-top">

    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Sidebar -->
        <ul class="navbar-nav bg-gradient-warning sidebar sidebar-dark accordion" id="accordionSidebar">

            <!-- Sidebar - Brand -->
            <a class="sidebar-brand d-flex align-items-center justify-content-center" href="index.html">
                <div class="sidebar-brand-icon rotate-n-15"></div>

                <div class="sidebar-brand-text mx-3">Raythos Aerospace</div>
            </a>

            <!-- Divider -->
            <hr class="sidebar-divider my-0">

            <li class="nav-item active">
                <a class="nav-link" asp-controller="AdminDashboard" asp-action="Index">
                    <i class="fas fa-fw fa-tachometer-alt"></i>
                    <span>Dashboard</span>
                </a>
            </li>

            <li class="nav-item active">
                <a class="nav-link" asp-controller="Products" asp-action="Index">
                    <i class="fas fa-box product-icon"></i>
                    <span>Product</span>
                </a>
            </li>

            <li class="nav-item active">
                <a class="nav-link" asp-controller="Inventories" asp-action="Index">
                    <i class="fas fa-cubes"></i>
                    <span>Inventory</span>
                </a>
            </li>

            <li class="nav-item active">
                <a class="nav-link" asp-controller="Orders" asp-action="Index">
                    <i class="fas fa-warehouse"></i>
                    <span>Order</span>
                </a>
            </li>

            <li class="nav-item active">
                <a class="nav-link" asp-controller="Users" asp-action="Index">
                    <i class="fas fa-users"></i>
                    <span>User</span>
                </a>
            </li>

            <li class="nav-item active">
                <a class="nav-link" asp-controller="Payments" asp-action="Index">
                    <i class="fas fa-credit-card"></i>
                    <span>Payment</span>
                </a>
            </li>
            <li class="nav-item active">
                <a class="nav-link" asp-controller="AppRoles" asp-action="Index">
                    <i class="fas fa-users"></i>
                    <span>Roles Management</span>
                </a>
            </li>

        </ul>
        <!-- End of Sidebar -->
        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                <!-- Topbar -->
                <nav class="navbar navbar-expand navbar-light bg-white topbar mb-4 static-top shadow">

                    <partial name="/Views/Shared/_LoginPartial.cshtml" />

                </nav>
                <!-- End of Topbar -->
                <!-- Begin Page Content -->
                <div class="container-fluid">

                    <!--  Create Role  -->
                    <div class="row">
                        <div class="col-md-4">
                            <form method="post">
                                <h2>Create Role</h2>
                                <hr />
                                <br /><br />
                                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                <div class="form-floating">
                                    <label asp-for="Name" class="control-label"></label>
                                    <input asp-for="Name" class="form-control" />
                                    <span asp-validation-for="Name" class="text-danger"></span>
                                </div>
                                <br /><br />
                                <button type="submit" class="btn btn-primary">Save</button>
                            </form>
                        </div>
                    </div>

                </div>
                <!-- /.container-fluid -->

            </div>
            <!-- End of Content Wrapper -->

        </div>
        <!-- End of Page Wrapper -->
        <!-- Bootstrap core JavaScript-->
        <script src="~/vendorAdmin/jquery/jquery.min.js"></script>
        <script src="~/vendorAdmin/bootstrap/js/bootstrap.min.js"></script>

        <!-- Core plugin JavaScript-->
        <script src="~/vendorAdmin/jquery-easing/jquery.easing.min.js"></script>

        <!-- Custom scripts for all pages-->
        <script src="~/js/js/sb-admin-2.min.js"></script>

</body>





</html>